{
  "buildCommand": "pnpm run build",
  "devCommand": "pnpm run dev",
  "installCommand": "pnpm install",
  "rewrites": [
    {
      "source": "/openapi/(.*)",
      "destination": "/api/proxy"
    }
  ],
  "env": {
    "OPENAI_API_BASE_URL": "@openai_api_base_url",
    "OPENAI_API_KEY": "@openai_api_key"
  }
}